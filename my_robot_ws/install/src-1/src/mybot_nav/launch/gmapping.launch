<?xml version="1.0"?>
<launch>
  
  <param name="/use_sim_time" value="false"/>
  <!--- Run gmapping -->
  <node pkg="gmapping" name="slam_gmapping" type="slam_gmapping" output="screen">
    <param name="delta" value="0.025"/>
    <param name="xmin" value="-25"/>
    <param name="xmax" value="25"/>
    <param name="ymin" value="-25"/>
    <param name="ymax" value="25"/>
    
    
    <param name="odom_frame" value="odom"/>
    <param name="base_frame" value="chassis"/>
    <param name="map_frame" value="map"/>

    <param name="linearUpdate" value="0.3"/>
    <param name="angularUpdate" value="1.0"/>
    <param name="temporalUpdate" value="2.0"/>
    <param name="map_update_interval" value="5.0"/>
    <param name="resampleThreshold" value="0.5"/>
    <param name="particles" value="25"/>
    
    
    <param name="maxUrange" value="3.49"/>
    <param name="sigma" value="0.05"/>
    <param name="kernelSize " value="1.0"/>
    <param name="lstep" value="0.025"/>
    <param name="astep" value="0.025"/>
    <param name="lsigma" value="0.075"/>
    <param name="iterations " value="7.0"/>
    <param name="lskip" value="0.0"/>
    <param name="ogain" value="3.0"/>  
    <param name="minimumScore" value="0.0"/> 
    
    <param name="llsamplerange" value="0.005"/>
    <param name="llsamplestep" value="0.005"/>
    <param name="lasamplerange" value="0.0025"/>  
    <param name="lasamplestep" value="0.0025"/> 
    <param name="transform_publish_period" value="0.05"/> 
    <param name="occ_thresh" value="0.25"/> 
    

   

  </node>


</launch>
