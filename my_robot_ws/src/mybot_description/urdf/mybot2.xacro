<?xml version='1.0'?>

<!-- This is a URDF description file of a mobile robot, written by Muhammad Asem, m7mad3asem@gmail.com
It models: 1-Chassis with Caster Wheel 2-Left wheel 3-right Wheel  4-Kinect /-->


<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">


<!-- +++++++++++++++++++++++Robot Definition+++++++++++++++++++++++++ /-->



<!--Chassis & Caster Link /-->

 <link name='chassis'>

   <visual name='chassis_visual'>

      <origin xyz="0 0 0" rpy=" 0 0 0"/>

      <geometry>
        <box size=".6 .5 .136"/>
      </geometry>

    </visual>    
    
  
   <inertial>

      <mass value="10.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />

   </inertial>
  

   <collision name='collision'>
      <geometry>
        <box size=".6 .5 .136"/>
      </geometry>
   </collision>



 <collision name='caster_collision'>
    <origin xyz="-0.25 0 -0.068" rpy=" 0 0 0"/>

    <geometry>
        <sphere radius="0.0402"/>
    </geometry>

    <surface>

        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>

     </surface>

 </collision>

<visual name='caster_visual'>

     <origin xyz="-0.25 0 -0.068" rpy=" 0 0 0"/>

     <geometry>
      <sphere radius="0.0402"/>
     </geometry>
     

</visual>

 <collision name='caster2_collision'>
    <origin xyz="0.25 0 -0.068" rpy=" 0 0 0"/>

    <geometry>
        <sphere radius="0.0402"/>
    </geometry>

    <surface>

        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>

     </surface>

 </collision>

<visual name='caster2_visual'>

     <origin xyz="0.25 0 -0.068" rpy=" 0 0 0"/>

     <geometry>
      <sphere radius="0.0402"/>
     </geometry>
     

</visual>


</link>


<!-- ++++++++++++++++Left wheel+++++++++++++++++++++++++++++++ /-->


<!--left wheel Joint /-->

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.1895 -0.0317" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>



<!--left wheel Link /-->

 <link name="left_wheel">

    <visual name="left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0765" length="0.035"/>
      </geometry>
    </visual>


    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0765" length="0.035"/>
      </geometry>
    </collision>

    

    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.0765" h="0.035"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>

 </link>

<!-- +++++++++++++++++++ Right Wheel +++++++++++++++++++++++++++++++++ /-->

<!--Right wheel joint/-->
  <joint type="continuous" name="right_wheel_hinge">

    <origin xyz="0 -0.1895 -0.0317" rpy="0 0 0"/>
    
    <child link="right_wheel"/>
    <parent link="chassis"/>

    <axis xyz="0 1 0" rpy="0 0 0"/>

    <limit effort="100" velocity="100"/>

    <joint_properties damping="0.0" friction="0.0"/>

  </joint>

<!--Right Wheel link /-->

  <link name="right_wheel">

    <visual name="right_wheel_visual">

      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>

      <geometry>
        <cylinder radius="0.0765" length="0.035"/>
      </geometry>

    </visual>

    
    <collision name="collision">

      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>

      <geometry>
        <cylinder radius="0.0765" length="0.035"/>
      </geometry>

    </collision>


    <inertial>

      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.0765" h="0.035"/>

      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
      </inertial>

  </link>
  
  <!-- ++++++++++++++++++  Upper level  +++++++++++++++++++++++++++++ /-->
<!--upper level joint /-->

<joint name="upper_level_joint" type="fixed">

    <origin xyz="0 0 0.206" rpy="0 0 0"/>

    <parent link="chassis"/>
    <child link="upper_level"/>

</joint>


<!--upper level  Link /-->

<link name="upper_level">
	<visual>
            <geometry>
        	<box size=".6 .5 .136"/>
      	    </geometry>
            <material name="red"/>
        </visual>
        <inertial>

      <mass value="10.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />

   </inertial>
  

   <collision name='collision'>
      <geometry>
        <box size=".6 .5 .136"/>
      </geometry>
   </collision>

</link>

<!-- ++++++++++++++++++  rplidar  +++++++++++++++++++++++++++++ /-->
<!--rplidar joint /-->

<joint name="rplidar_joint" type="fixed">

    <origin xyz="0.065 0 0.098" rpy="0 0 0"/>

    <parent link="chassis"/>
    <child link="rplidar_link"/>

</joint>


<!--rplidar  Link /-->

<link name="rplidar_link">
	<visual>
            <geometry>
                <cylinder radius="0.05" length="0.06"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.06"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.06" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
</link>

